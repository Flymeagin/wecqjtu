page {
  width: 100%;
  height: 100%;
 background-color: #f2f2f2;
}

.login-container {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.banner {
  width: 100%;
  background-color: #1275c3;
  height: 110rpx;
  text-align: center;
}

.banner image {
  width: 284rpx;
  height: 110rpx;
}
.content{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 15rpx;
}
.content .title{
    width: 100%;
    height: 70rpx;
    line-height: 70rpx;
    /* border-left: 15rpx solid #4169E1; */
    background-color: white;
}
.content .title text{
  color:#1275c3;
  display: inline-block;
  margin-left: 3%;
  font-size: 32rpx;
}
navigator{
  width: 100%;
  background-color: white;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.content .news{
  width: 92%;
  height: 200rpx;
  display: flex;
  margin-top: 0px auto;
  flex-direction: column;
  justify-content: space-around;
  border-bottom: 1px solid #dcdcdc;
}
.content .news .neirong{
  font-size: 35rpx;
}
.content .news .time{
  font-size: 29rpx;
  color:#666;
}
.caozuo{
  width: 100%;
  height: 70rpx;
  font-size: 30rpx;
  color: #666;
  text-align: center;
  line-height: 70rpx;
  background-color: white
}